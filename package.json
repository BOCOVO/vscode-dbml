{
    "name": "vscode-dbml",
    "displayName": "vscode-dbml",
    "description": "Language support for the Database Markup Language",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "repository": {
        "type" : "git",
        "url" : "https://github.com/mattmeyers/vscode-dbml.git"
    },
    "contributes": {
        "languages": [{
            "id": "dbml",
            "aliases": ["DBML", "dbml", "Database Markup Language"],
            "extensions": [".dbml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dbml",
            "scopeName": "source.dbml",
            "path": "./syntaxes/dbml.tmLanguage.json"
        }],
        "snippets": [
            {
              "language": "dbml",
              "path": "./snippets/dbml.json"
            }
          ]
    }
}
